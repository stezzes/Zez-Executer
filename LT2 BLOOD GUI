Treeaura.Name = "Tree aura"
Treeaura.Parent = WoodFrame
Treeaura.BackgroundColor3 = Color3.new(0, 0, 0)
Treeaura.BackgroundTransparency = 0.59999998807907
Treeaura.BorderColor3 = Color3.new(1, 0, 0)
Treeaura.Position = UDim2.new(0, 3, 0, 213)
Treeaura.Size = UDim2.new(0, 165, 0, 20)
Treeaura.Font = Enum.Font.Fantasy
Treeaura.Text = "Tree Aura"
Treeaura.TextColor3 = Color3.new(1, 1, 1)
Treeaura.TextSize = 14
Treeaura.MouseButton1Down:connect(function()
  makesound("rbxassetid://156785206")
    if CutEnabled == true then
        CutEnabled = false
        Treeaura.Text = "Tree Aura off"
Treeaura.TextColor3 = Color3.new(255, 0, 0)
    elseif CutEnabled == false then
        CutEnabled = true
        Treeaura.Text = "Tree Aura on"
Treeaura.TextColor3 = Color3.new(0, 1, 0)
    end
end)
