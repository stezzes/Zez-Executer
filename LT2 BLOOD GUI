MaxLand.Name = "MaxLand"
MaxLand.Parent = PlayerFrame
MaxLand.BackgroundColor3 = Color3.new(0, 0, 0)
MaxLand.BackgroundTransparency = 0.69999998807907
MaxLand.BorderColor3 = Color3.new(1, 0, 0)
