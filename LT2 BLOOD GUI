  makesound("rbxassetid://156785206")
	MainFrame:TweenPosition(UDim2.new(0.5, -40, 0, -28), "In", "Back",2.5)
	wait(2.5)
	game:GetService("StarterGui"):SetCore("SendNotification",{
Title = "ODDYS BLOOD GUI",
Text = "Thanks for using!",
Duration = 6,
	})
BLOOD:destroy()
VBlood:destroy()
end)

